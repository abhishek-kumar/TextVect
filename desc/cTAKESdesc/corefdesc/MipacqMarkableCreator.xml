<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>org.chboston.cnlp.ctakes.coref.uima.ae.MipacqMarkableCreator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>MarkableCreator</name>
    <description>Create Markables from NEs and Pronouns</description>
    <version>1.0</version>
    <vendor>Mayo Clinic</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>modalAdj</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>cogved</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>otherVerb</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>modalAdj</name>
        <value>
          <array>
            <string>necessary</string>
            <string>good</string>
            <string>economical</string>
            <string>possible</string>
            <string>useful</string>
            <string>easy</string>
            <string>certain</string>
            <string>advisable</string>
            <string>desirable</string>
            <string>likely</string>
            <string>convenient</string>
            <string>difficult</string>
            <string>important</string>
            <string>sufficient</string>
            <string>legal</string>
            <string>worthwhile</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>cogved</name>
        <value>
          <array>
            <string>recommended</string>
            <string>thought</string>
            <string>believed</string>
            <string>known</string>
            <string>anticipated</string>
            <string>assumed</string>
            <string>expected</string>
            <string>noted</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>otherVerb</name>
        <value>
          <array>
            <string>seem</string>
            <string>appear</string>
            <string>mean</string>
            <string>follow</string>
          </array>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="type-system/CorefTypes.xml"/>
        <import location="../common-type-system/desc/common_type_system.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <externalResourceDependencies>
    <externalResourceDependency>
      <key>InclusionCondition</key>
      <description>Condtions that an Annotation must satisfy in order to be considered later as a potential participant of coreference pairs.</description>
      <optional>true</optional>
    </externalResourceDependency>
  </externalResourceDependencies>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
