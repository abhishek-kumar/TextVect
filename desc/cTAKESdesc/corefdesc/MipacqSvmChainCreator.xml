<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>org.chboston.cnlp.ctakes.coref.uima.ae.MipacqSvmChainCreator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>SvmChainCreator</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters/>
    <configurationParameterSettings/>
    <typeSystemDescription>
      <imports>
        <import location="type-system/CorefTypes.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <externalResourceDependencies>
    <externalResourceDependency>
      <key>svmPronModel</key>
      <description>pronoun model file</description>
      <interfaceName>edu.mayo.bmi.uima.core.resource.FileResource</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>svmDemModel</key>
      <description>demonstrative and relative pronoun model file</description>
      <interfaceName>edu.mayo.bmi.uima.core.resource.FileResource</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>svmAnaphoricityModel</key>
      <description>anaphoricity classifier model file</description>
      <interfaceName>edu.mayo.bmi.uima.core.resource.FileResource</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>svmCorefModel</key>
      <description>coreference classifier model file</description>
      <interfaceName>edu.mayo.bmi.uima.core.resource.FileResource</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>stopWords</key>
      <description>snowball English stop words list</description>
      <interfaceName>edu.mayo.bmi.uima.core.resource.FileResource</interfaceName>
      <optional>true</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>frags</key>
      <description>File of tree kernel fragment features</description>
      <interfaceName>edu.mayo.bmi.uima.core.resource.FileResource</interfaceName>
      <optional>true</optional>
    </externalResourceDependency>
  </externalResourceDependencies>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>svmPronModelFile</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:corefresources/pronoun.mayo.rbf.model</fileUrl>
        </fileResourceSpecifier>
        <implementationName>edu.mayo.bmi.uima.core.resource.FileResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>svmDemModelFile</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:corefresources/dem.model</fileUrl>
        </fileResourceSpecifier>
        <implementationName>edu.mayo.bmi.uima.core.resource.FileResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>svmAnaphoricityModelFile</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:corefresources/anaphoricity.mayo.rbf.model</fileUrl>
        </fileResourceSpecifier>
        <implementationName>edu.mayo.bmi.uima.core.resource.FileResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>svmCorefModelFile</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:corefresources/ne.mayo.rbf.model</fileUrl>
        </fileResourceSpecifier>
        <implementationName>edu.mayo.bmi.uima.core.resource.FileResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>StopWordsFile</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:corefresources/stop.txt</fileUrl>
        </fileResourceSpecifier>
        <implementationName>edu.mayo.bmi.uima.core.resource.FileResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>fragsFile</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:corefresources/frags.txt</fileUrl>
        </fileResourceSpecifier>
        <implementationName>edu.mayo.bmi.uima.core.resource.FileResourceImpl</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>svmPronModel</key>
        <resourceName>svmPronModelFile</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>svmDemModel</key>
        <resourceName>svmDemModelFile</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>svmAnaphoricityModel</key>
        <resourceName>svmAnaphoricityModelFile</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>stopWords</key>
        <resourceName>StopWordsFile</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>svmCorefModel</key>
        <resourceName>svmCorefModelFile</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>frags</key>
        <resourceName>fragsFile</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
